<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="start" android:id="@+id/linearMenu"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearOne"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_77pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh"
            android:paddingTop="@dimen/_77pxv" style="@style/groupStyle{&quot;name&quot;:&quot;gray_100&quot;}">
            <ImageView android:id="@+id/imageCircleImage"
                android:layout_height="@dimen/_85pxh"
                android:layout_width="@dimen/_85pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_ellipse_108}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_108"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSuzaneJobs"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_suzane_jobs"
                style="@style/txtOverpassbold24" tools:text="@string/lbl_suzane_jobs"/>
            <TextView android:ellipsize="end" android:id="@+id/txtEmail"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_user_gmail_com"
                style="@style/txtOverpassmedium14" tools:text="@string/lbl_user_gmail_com"/>
            <LinearLayout android:id="@+id/linearFrame"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageLock"
                    android:layout_gravity="bottom"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_lock"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
                <TextView android:ellipsize="end" android:gravity="top"
                    android:id="@+id/txtEditProfile"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/lbl_edit_profile"
                    style="@style/txtOverpassmedium16" tools:text="@string/lbl_edit_profile"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearTransaction"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageClock"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_clock"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_clock"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:id="@+id/txtTransactionHistory"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/lbl_history_8"
                    style="@style/txtOverpassmedium16" tools:text="@string/lbl_history_8"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearSettings"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageSearch"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_search"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:id="@+id/txtNotificationsSettin"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/msg_notifications_settings"
                    style="@style/txtOverpassmedium16" tools:text="@string/msg_notifications_settings"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearFavoriteFood"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageFavorite"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_favorite_onprimarycontainer"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_favorite_onprimarycontainer"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:id="@+id/txtFavoriteOutfits"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/lbl_share_app"
                    style="@style/txtOverpassmedium16" tools:text="@string/lbl_share_app"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
