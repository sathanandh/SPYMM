<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearOnboarding"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:paddingEnd="@dimen/_24pxh"
        android:paddingStart="@dimen/_24pxh" style="@style/groupStyle{&quot;name&quot;:&quot;gray_100&quot;}">
        <ImageView android:id="@+id/imageThumbsUp"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_80pxh"
            android:layout_width="@dimen/_78pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_thumbs_up"
            tools:ignore="ContentDescription" tools:src="@drawable/img_thumbs_up"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtOnlineDigital"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@string/msg_online_digital"
            style="@style/txtOverpassmedium24" tools:text="@string/msg_online_digital"/>
        <TextView android:ellipsize="end" android:id="@+id/txtBanking"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:singleLine="true" android:text="@string/lbl_banking"
            style="@style/txtOverpassbold48" tools:text="@string/lbl_banking"/>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtWeHelpOurUser"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:text="@string/msg_we_help_our_user"
            style="@style/txtOverpassregular16_2" tools:text="@string/msg_we_help_our_user"/>
        <androidx.appcompat.widget.AppCompatButton
            android:background="@color/{&quot;name&quot;:&quot;primary&quot;}"
            android:ellipsize="end" android:fontFamily="@font/overpass"
            android:gravity="center" android:id="@+id/btnStartNow"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_58pxv"
            android:layout_width="@dimen/_366pxh"
            android:paddingBottom="@dimen/_16pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_16pxv" android:singleLine="true"
            android:text="@string/lbl_start_now"
            android:textAllCaps="true"
            android:textColor="@color/{&quot;name&quot;:&quot;onPrimary&quot;}"
            android:textFontWeight="700"
            android:textSize="@dimen/_20pxh" android:textStyle="bold" tools:text="@string/lbl_start_now"/>
    </LinearLayout>
</layout>
